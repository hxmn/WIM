    //********* Start Generic Comboboxes Events ***********//
    //Allow user to be able to type, but not select anything that is not available in the item list//
    function ddlCollapse(sender, eventArgs) {
        if (sender.get_text() == "") {
            if (sender.get_id() == "ctl00_ctl00_ContentPlaceHolder1_ddlSource") {
                sender.focus();
            }
            return;
        }

        var i = 0;
        var item;
        while (sender.getItem(i)) {
            item = sender.getItem(i);
            if (sender.get_text() == item.get_text()) {
                sender.selectItem(item);
                //sender.filter(sender.get_text());

                switch (sender.get_id()) {
                    case "ctl00_ctl00_ContentPlaceHolder1_ddlSource":
                        document.getElementById("ctl00_ctl00_ContentPlaceHolder1_lblSource2val").innerText = item.Text;
                        document.getElementById("ctl00_ctl00_ContentPlaceHolder1_lblSource3val").innerText = item.Text;
                        break;
                    case "ctl00_ctl00_ContentPlaceHolder1_ddlCountry":
                        document.getElementById("ctl00_ctl00_ContentPlaceHolder1_lblCountry2val").innerText = item.Text;
                        document.getElementById("ctl00_ctl00_ContentPlaceHolder1_lblCountry3val").innerText = item.Text;
                        ddlCounty.set_text("");
                        ddlCounty.disable();

                        ddlProvState.FilterString = "";
                        ddlProvState.CallbackFilterString = "";
                        ddlProvState.set_text("");
                        ddlProvState.disable();
                        ddlProvState.filter(item.get_value());

                        ddlBasinName.FilterString = "";
                        ddlBasinName.CallbackFilterString = "";
                        ddlBasinName.set_text("");
                        ddlBasinName.disable();
                        ddlBasinName.filter(item.get_value());

                        break;
                    case "ctl00_ctl00_ContentPlaceHolder1_ddlWellProfileType":
                        var vertical = "V";
                        var disableTVD = (item.Value == vertical);
                        document.getElementById("ctl00_ctl00_ContentPlaceHolder1_txtPlugbackTVD").disabled = disableTVD;
                        document.getElementById("ctl00_ctl00_ContentPlaceHolder1_txtMaxTVD").disabled = disableTVD;
                        break;
                }
                return;
            }
            i++;
        }
        sender.set_text("");
        sender.toggleExpand();
        sender.focus();
    }
