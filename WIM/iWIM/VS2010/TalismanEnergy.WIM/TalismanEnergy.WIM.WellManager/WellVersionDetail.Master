            selItem = document.getElementById('ctl00_ctl00_ContentPlaceHolder1_nodeDlgLocationAccuracy');
            cell = row.getMember('LOCATION_ACCURACY');
            try { row.SetValue(cell.get_column().get_columnNumber(), selItem.value); } catch (err) { }

            selItem = ctl00_ctl00_ContentPlaceHolder1_ddlNodeDatum.getSelectedItem();
            cell = row.getMember('GEOG_COORD_SYSTEM_ID');
            try { row.SetValue(cell.get_column().get_columnNumber(), selItem.get_value()); } catch (err) { }
            try { cell.SetProperty('Text', selItem.get_text()); } catch (err) { }

            selItem = document.getElementById('ctl00_ctl00_ContentPlaceHolder1_nodeDlgRemark');
