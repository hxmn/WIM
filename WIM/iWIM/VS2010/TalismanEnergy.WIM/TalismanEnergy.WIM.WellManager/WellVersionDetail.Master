    var entityMap = {
        "&": "&amp;",
        "<": "&lt;",
        ">": "&gt;",
        '"': '&quot;',
        "'": '&#39;',
        "/": '&#x2F;'
    };

    function escapeHtml(string) {
        return String(string).replace(/[&<>"'\/]/g, function (s) {
            return entityMap[s];
        });
    }
    function setTooltip(element, text) {
        var safeText = escapeHtml(escapeHtml(text)).replace(/\n/g, '<br />');
        $(element).mouseover(function () { tooltip.show(safeText); });
        $(element).mouseout(function () { tooltip.hide(); });
    }
    function setContentAndTooltip(text) {
        var safeText = escapeHtml(escapeHtml(text)).replace(/\n/g, '<br />');
        var span = '<span style="display:inline-block;width:100%;" ';
        //span += 'onmouseover="tooltip.show(\'' + text.replace(/\n/g, '<br />').replace(/\'/g, '\\\'') + '\');" ';
        span += 'onmouseover="tooltip.show(\'' + safeText + '\');" ';
        span += 'onmouseout="tooltip.hide();" >';
        span += text;
        span += '</span>';
        return span;
    }
