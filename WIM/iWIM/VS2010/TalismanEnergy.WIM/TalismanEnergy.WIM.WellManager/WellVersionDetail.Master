<script type="text/javascript">
    function DataGrid1_onItemSelect(sender, eventArgs)
    { updateRemoveBtnState(); }

    function DataGrid1_onItemUnSelect(sender, eventArgs)
    { updateRemoveBtnState(); }

    function updateRemoveBtnState() {
        var locs = DataGrid1.getSelectedItems();
        var nLocs = 0;
        if (locs)
        { nLocs = locs.length; }
        if (nLocs < 1)
        { $("#ctl00_ctl00_ContentPlaceHolder1_btnRemoveRows").attr('disabled', 'disabled').addClass("is-disabled"); }
        else
        { $("#ctl00_ctl00_ContentPlaceHolder1_btnRemoveRows").removeAttr('disabled').removeClass("is-disabled"); }
    }

    function DataGrid1_onItemDoubleClick(sender, eventArgs) {
        if ($("#ctl00_ctl00_ContentPlaceHolder1_GridRowsControlsBar").is(':visible')) {
            var row = eventArgs.get_item();
            showEditNodeDialog(row.ClientId);
        }
    }

    function showEditNodeDialog(rowId) {
        //DataGrid1.edit(DataGrid1.getItemFromClientId(rowId))
        document.getElementById('nodeDlgRowId').value = rowId;

        var item = ctl00_ctl00_ContentPlaceHolder1_DataGrid1.getItemFromClientId(rowId);

        document.getElementById('ctl00_ctl00_ContentPlaceHolder1_nodeDlgObsNo').value = item.getMember('NODE_OBS_NO').get_value();

        var ddl;
        var value;
        ddl = ctl00_ctl00_ContentPlaceHolder1_ddlNodePos;
